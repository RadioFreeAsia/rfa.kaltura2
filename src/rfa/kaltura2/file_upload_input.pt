<html>
  <div id="uploadHook" />
  <div id="uploadbox1">
    <div class="relative">
      <div id="uploadbutton">
        <form id='fileupload'>
          <button id='fileupload-btn' class="fileinput-button">
          Choose a file to upload   		
            <input id="fileinput" class="fileinput" data-uploadboxid="1" type="file" name="fileData" accept="video/*">
            </button>
          </form>
        </div>
        <span id="upload-file-info" class="label"></span>
        <button class="cancelBtn hidden pull-right" title="Cancel" id="cancelBtn">Cancel</button>
      </div>

      <div id="progress" class="progress hidden">
        <div id="progressBar" class="bar active"></div>
        <div class="anchor">
          <div class="message"></div>
        </div>
      </div>

      <div id="successmsg" class="alert alert-success hidden text-center">
        <span class="large">
          <strong>Upload Completed</strong>
        </div>

      </div>
      <div id="entry_details" class="entry_details hidden"></div>
      <input id="kaltura-upload-token-id" hidden></input>
      <script async src="++plone++rfa.kaltura2/js/kalturaFileUploader.js"></script>
    </html>
